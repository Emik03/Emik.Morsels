                <Virtuosity />
                <JetBrainsAnnotations />
                <LocalsInit Default="false" />
                <Caseless StringComparison="InvariantCulture" />
                <InlineMethod />
                <Divorce />
                <Absence />
            </Weavers>
        </WeaverConfiguration>
    </PropertyGroup>
    <PropertyGroup>
